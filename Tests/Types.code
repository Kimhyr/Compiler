procedure Initiate {
	\\ Signed Integers
	let int8::Int8 	 = 1;
	let int16::Int16 = 2;
	let int32::Int32 = 3;
	let int64::Int64 = 4;
	
	\\ Unsigned Integers
	let uInt8::UInt8   = 1;
	let uInt16::UInt16 = 2;
	let uInt32::UInt32 = 3;
	let uInt64::UInt64 = 4;

	\\ Floats
	let float32::Float32 = 0.32;
	let float64::Float64 = 0.32;

	\\ Aliases
	alias Char8 = UInt8;
	alias Char16 = UInt16;
	
	alias Bit8 = UInt8;
	alias Bit16 = UInt8;
	
	$switch _ARCH_ {
		case _ARCH_X86_64_ =>
			alias USize = UInt64;
		case =>
			alias USize = UInt32;
	}
	
}
