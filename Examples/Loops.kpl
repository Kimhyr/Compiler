procedure For(n :: UInt64) {
  datum i :: ?UInt64 = 0;
  label Start {
    if i > n => goto End;
    i += 1;
    goto Start;
  }
  label End;

  \\ =: onward
  \\ := backward
  \\ :=: backward and onward
  \\ {} scoped

  datum c :: U8;
label WHILE_START =:
	c = Get;
	if == EoF => jumpto WHILE_END;


	jumpto WHILE_START;
label WHILE_END :=
}
