Unsigned := <'0'..'9'> [<'0'..'9'>|'_']*
Signed := ['+'|'-']? Unsigned

Integer := [Signed|Unsigned]

Real := Signed '.' [<'0'..'9'>|'_']*
Scientific := Signed ['e'|'E'] ['+'|'-']? <'0'..'9'>+

Float := [Real|Scientific]

Binary := '0' ['b'|'B'] ['0'|'1'|'_']+
Hexadecimal := '0' ['x'|'X'] [<'0'..'9'>|<'a'..'f'>|<'A'..'F'>|'_']+

Machine := [Binary|Hexadecimal]

Symbol := '$'? '"' <$^> '"'
String := '$'? '"' <$^>+ '"'

Text := [Symbol|String]
