Integer := ['+'|'-']? <'0'..'9'> [<'0'..'9'>|'_']*

Real := ['+'|'-']? <'0'..'9'> [<'0'..'9'>|'_']* '.' [<'0'..'9'>|'_']*
Scientific := ['+'|'-']? <'0'..'9'> [<'0'..'9'>|'_']* ['e'|'E'] ['+'|'-']? <'0'..'9'>+

Binary := '0' ['b'|'B'] ['0'|'1'|'_']+
Hexadecimal := '0' ['x'|'X'] [<'0'..'9'>|<'a'..'f'>|<'A'..'F'>|'_']+

Symbol := '$'? '"' <$^> '"'
String := '$'? '"' <$^>+ '"'
