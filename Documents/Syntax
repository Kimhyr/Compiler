Structure
    : 'structure' <Identifier> ('<' <TemplateInput>)? ['=' <Structure>|'{' <Body>|';']

Procedure
    : 'procedure' <Identifier> ('<' <TemplateInput>)? ('(' <ParameterInput>)? ('->' <TypeUse>)? ['=' <Procedure>|'{' <Body>|';']

Datum
    : 'datum' <Identifier> ':' <TypeUse> ['=' <Datum>|'{' <Body>|';']
