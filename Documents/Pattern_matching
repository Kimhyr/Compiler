enumerare Token::Nat8 {
    Identifier::@Nat8,
    Literal::enumerare Literal::Nat8 {
        None::Nat8,
        String::@Nat8,
        Symbol::Nat8,
        Natural::Nat64,
        Integer::Int64,
        Real::Real64,
    };
}

label Token =: {
    structure Identifier {
        \\ "''" : Constant
        ''TOKEN_ENUMERATOR::Nat8 = 1,
        value::@Nat8,
    }
}

import Core:System:IO;

procedure MatchToken(token::Token) {
    switch token {
        case Token.Identifier[identifier] {

        }
        case Token.Literal[literal] {
            switch literal {
                case Token.Literal.None =>
                    throw Error;
                case Token.Literal.String =>

            }
        }
        else {
            throw Exceptions:InvalidArgument("");
        }
    }
}
