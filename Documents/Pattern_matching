enumerare Token::UInt8 {
    Identifier::@UInt8,
    Literal::enumerare Literal::UInt8 {
        None::UInt8,
        String::@UInt8,
        Symbol::UInt8,
        Natural::UInt64,
        Integer::Int64,
        Real::Float64,
    };
}

label Token =: {
    structure Identifier {
        \\ "''" : Constant
        ''TOKEN_ENUMERATOR::UInt8 = 1,
        value::@UInt8,
    }
}

import Core:System:IO;

procedure MatchToken(token::Token) {
    switch token {
        case Token.Identifier[identifier] {

        }
        case Token.Literal[literal] {
            switch literal {
                case Token.Literal.None =>
                    throw Error;
                case Token.Literal.String =>

            }
        }
        else {
            throw Exceptions:InvalidArgument("");
        }
    }
}
