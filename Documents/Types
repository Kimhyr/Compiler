procedure Types {
label Integers =:
    datum _::Int8;
    datum _::Int16;
    datum _::Int32;
    datum _::Int64;

    datum _::Nat64;
    datum _::Nat16;
    datum _::UInt32;
    datum _::Nat64;

label Floats =:
    datum _::Float32;
    datum _::Float64;

label Abstracts =:
    datum _::Ptr<TYPE>;
    datum _::Array<SIZE, TYPE>;
}
